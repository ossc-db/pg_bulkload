CREATE TABLE binout1 (
  val1 smallint NOT NULL,
  val2 integer NOT NULL,
  val3 bigint NOT NULL,
  val4 integer NOT NULL,
  val5 bigint NOT NULL,
  val6 real NOT NULL,
  val7 double precision NOT NULL,
  val8 text NOT NULL
);
